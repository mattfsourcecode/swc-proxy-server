{
  "name": "swc-proxy-server",
  "version": "1.0.0",
  "description": "Template code for a reusable Express.js server that proxies requests to a specified API.",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'node -r @swc-node/register' src/server.ts",
    "start": "node dist/server.js",
    "build": "swc src -d dist"
  },
  "keywords": [
    "swc",
    "express",
    "api",
    "proxy"
  ],
  "author": "matt-development-work",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "helmet": "^7.2.0",
    "http-proxy-middleware": "^2.0.7"
  },
  "devDependencies": {
    "@swc-node/register": "^1.10.9",
    "@swc/cli": "^0.1.65",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "nodemon": "^3.1.7"
  }
}
